<h1>Users: </h1>



<h4>Search by Username:</h4>
<%= form_tag(users_path, method: :get) do%>
  <%= text_field_tag(:search, params[:search]) %>
  <%= submit_tag ("Search") %>
<% end %>
<br>

<% if params[:search] %>
<ol>
<%@user_search.each do |user|%>
<li><%= link_to user.username, profile_path(user) %> </li>
<%end%>
</ol>
<% end %>
